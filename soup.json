{
    "logging": {
        "level": "debug",
        "format": "logger:stdout?json=false"
    },
    "soup": {
        "version": {
            "format": "json"
        },
        "sync": {
            "k8s": {
                "path": "sync_k8s_path",
                "context": "sync_k8s_context"
            },
            "alert": {
                "uri": "notifier:opsgenie?host=api.opsgenie.com&apikey=123456-1234-4321-7890-098765432109",
                "teams": ["teamA", "teamB"],
                "priority": "P3",
                "tags": ["tagA", "tagB", "tagC"],
                "enable": false
            },
            "actuator": {
                "address": ":9081",
                "root": "/actuators"
            },
            "repo": {
                "vcs": {
                    "uri": "svc:git?username=user-repo&token=token-repo",
                    "secret": "repo-secret"
                }                
            },
            "serve": {
                "vcs": {
                    "uri": "svc:git?username=user-serve&token=token-serve",
                    "secret": "serve-secret"
                }
            }
        },
        "kubewatch": {
            "alert": {
                "uri": "notifier:console",
                "teams": ["teamA", "teamB"],
                "priority": "P3",
                "tags": ["tagA", "tagB"],
                "enable": false
            },
            "actuator": {
                "address": ":9081",
                "root": "/actuators"
            },
            "informer": {
                "namespaces": ["origo-devops-stg"],
                "resources": "v1/pods,v1/services,apps/v1/deployments,apps/v1/statefulsets,networking.k8s.io/v1/ingresses,v1/persistentvolumeclaims,v1/configmaps,apps/v1/daemonsets,batch/v1/cronjobs,batch/v1/jobs,rbac.authorization.k8s.io/v1/roles,rbac.authorization.k8s.io/v1/rolebindings",
                "uri": "informer:k8s?context=sre-aws-stg&path=/Users/fraildan/.kube/config&resync=45s&mode=diff"
            }
        },
        "kubediff": {
            "alert": {
                "uri": "notifier:console",
                "teams": ["teamA", "teamB"],
                "priority": "P3",
                "tags": ["tagA", "tagB"],
                "enable": false
            },
            "actuator": {
                "address": ":9081",
                "root": "/actuators"
            },
            "informer": {
                "namespaces": ["origo-devops-stg"],
                "resources": "v1/pods,v1/services,apps/v1/deployments,apps/v1/statefulsets,networking.k8s.io/v1/ingresses,v1/persistentvolumeclaims,v1/configmaps,apps/v1/daemonsets,batch/v1/cronjobs,batch/v1/jobs,rbac.authorization.k8s.io/v1/roles,rbac.authorization.k8s.io/v1/rolebindings",
                "uri": "informer:k8s?context=sre-aws-stg&path=/Users/fraildan/.kube/config&resync=45s&mode=diff"
            }
        }
    }
}
